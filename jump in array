
```
#include<bits/stdc++>

void main()
{

vector<int> a;

cout<<"enter the array length"<<endl;
cin>>n;
cout<<"enter the array elements"<<endl;
for(int i=0;i<n;i++)
{
   cin>>a[i];
   }
   
  int r=0;
  for(i=0;i<=r;i<n;i++)
  {
  r=max(i+a[i],r);
  }
  if(i==n)
  cout<<"possible<<endl;
  else
  cout<<"not possible"<<endl;
  
  }
  
