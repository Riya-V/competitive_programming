```

#include<bits/stdc++.h>

void main()
{
vector<int> a;
cout<<"enter the array length"<<endl;
cin>>n;
cout<<"enter the array elements"<<endl;
for(int i=0;i<n;i++)
{
   cin>>a[i];
   }
   
int x=a[0];
int y = 1;

for(int i=1;i<n;i++)
{
   
  x=x^a[i];
  }
for(i=2;i<n+1;i++)
{
y=y^i;
}

cout<<"the missing number is ="<<x^y<<endl;

}


```












