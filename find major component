```

#include<bits/stdc++.h>

using namespace std;

void main()
{
vector<int> a;
int n;
cout<<"enter the size of the vector"<<endl;
cin>>n;
cout<<"entre the array elemeents"<<endl;
for(int i=0;i<n;i++)
{
cin>>a[i];
}
int index=0;
int c=0;
for(int i=0;i<n;i++)
{
if(a[index]=a[i])
c++;
else
c--;
if(c==0)
{
index=i;
c=1;
}
}

cout<<"major element="<<a[index];
}

```
